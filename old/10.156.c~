#include<stdio.h>
#include"./str/str.h"
#include"./malloc/create.h"

main()
{
	
	char *data1;
	char *data2;
	int ret;
	
	data1 = (char *)create_arr(6);

	data2 = (char *)create_arr(12);

	strcpy(data1,"abcde");

	printf("String1 = %s\n",data1);

	ret = in_string(data1,data2);
	if(ERR == ret)
	{
		printf("拷贝失败！\n");
		return ERR;
	}
	printf("String2 = %s\n",data2);
	
	free(data1);
	free(data2);
}
